{
  "name": "tensorflow",
  "version": "0.0.1",
  "description": "Tensorflow Boilerplate",
  "main": "index.py",
  "scripts": {
    "script": "yarn start:script && yarn log:script",
    "start:script": "docker-compose -f docker-compose.script.yml up -d --build",
    "log:script": "docker logs -f tensorflow-script --since $(date \"+%s\")",
